<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.ur.mi.android.project_sunset">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/travel"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/travel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EntryScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondScreen"></activity>
        <activity android:name=".Calculator" />
        <activity android:name=".AddWaypointScreen"></activity>
    </application>

</manifest>